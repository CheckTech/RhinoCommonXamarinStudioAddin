<Addin id = "MonoDevelop.Debugger.Soft.Rhino"
       name	     = "Mono Soft Debugger Support for Rhinoceros"
       author	 = "Robert McNeel and Associates"
       url       = "http://www.irhino3d.com"
       description = "Mono Soft Debugger Support for Rhinoceros"
       copyright   = "2011 Robert McNeel and Associates"
       category = "Debugging"
       version   = "2.6">
	
		
	<Dependencies>
		<Addin id="MonoDevelop.Core" version="2.6"/>
		<Addin id="MonoDevelop.Debugger" version="2.6"/>
		<Addin id="MonoDevelop.Debugger.Soft" version="2.6"/>
	</Dependencies>

	<Extension path="/MonoDevelop/Debugging/DebuggerEngines">
		<DebuggerEngine
			id="Mono.Debugger.Soft.Rhino"
			name="Mono Soft Debugger for Rhinoceros"
			features="Breakpoints, Pause, Stepping, DebugFile, Catchpoints"
			type="MonoDevelop.Debugger.Soft.Rhino.RhinoSoftDebuggerEngine" />
	</Extension>
</Addin>