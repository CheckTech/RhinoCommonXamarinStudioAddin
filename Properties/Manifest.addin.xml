<?xml version="1.0" encoding="UTF-8"?>
<ExtensionModel>
  <Extension path="/MonoDevelop/Debugging/DebuggerEngines">
    <DebuggerEngine
      id="Mono.Debugger.Soft.Rhino"
      name="Mono Soft Debugger for Rhinoceros"
      features="Breakpoints, Pause, Stepping, DebugFile, Catchpoints"
      type="MonoDevelop.Debugger.Soft.Rhino.RhinoSoftDebuggerEngine" />
  </Extension>

  <Extension path = "/MonoDevelop/ProjectModel/ProjectServiceExtensions">
    <Class class = "MonoDevelop.Debugger.Soft.Rhino.RhinoProjectServiceExtension" />
  </Extension>

  <Extension path="/MonoDevelop/Ide/ProjectTemplates">
    <ProjectTemplate id="Rhino-Plugin" file="Templates/RhinoPlugin.xpt.xml" />
  </Extension>

  <Extension path="/MonoDevelop/Ide/FileTemplates">
    <FileTemplate id="Rhino-Command" file="Templates/Command.xft.xml" />
  </Extension>

</ExtensionModel>
